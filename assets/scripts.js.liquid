document.addEventListener("DOMContentLoaded", () => {
  let paginationControls = document.querySelector(".pagination-controls");

  if (paginationControls) {
    let prevBtn = paginationControls.querySelector("span.prev > a");
    let nextBtn = paginationControls.querySelector("span.next > a");

    prevBtn.textContent = "<";
    nextBtn.textContent = ">";
  }
});
